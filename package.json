{
  "name": "vscode-markdown-paste-image",
  "displayName": "Markdown Paste",
  "description": "A smartly paste for markdown.",
  "version": "1.1.1",
  "publisher": "telesoho",
  "author": {
    "name": "telesoho",
    "email": "telesoho@gmail.com"
  },
  "license": "(MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/telesoho/vscode-markdown-paste-image.git"
  },
  "homepage": "https://github.com/telesoho/vscode-markdown-paste-image",
  "bugs": {
    "url": "https://github.com/telesoho/vscode-markdown-paste-image/issues"
  },
  "icon": "res/images/icon.png",
  "galleryBanner": {
    "color": "#252526",
    "theme": "dark"
  },
  "keywords": [
    "paste",
    "image",
    "markdown",
    "paste image",
    "paste url",
    "paste code",
    "AI"
  ],
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension",
  "scripts": {
    "publish": "npx vsce publish",
    "vscode:prepublish": "npm run compile",
    "build:xclip": "cd xclip && npm run build && npm run test",
    "prettier": "npx prettier -w .",
    "compile": "npm run lint && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run build:xclip && rimraf ./out_test && npm run prettier && npm run lint && tsc --project ./ts-test.json && cpx \"./res/**/*\" \"./out_test/res\" --clean",
    "lint": "eslint src --ext ts",
    "test": "node ./out_test/test/runTest.js",
    "dev:preinstall": "npx vsce package",
    "dev:install": "code --install-extension ./vscode-markdown-paste-image-*.vsix"
  },
  "contributes": {
    "configuration": {
      "id": "MarkdownPaste",
      "title": "Markdown Paste Configuration",
      "properties": {
        "MarkdownPaste.enableAI": {
          "type": "boolean",
          "scope": "resource",
          "default": false,
          "description": "Enable AI to parse cliboard context"
        },
        "MarkdownPaste.aiKey": {
          "type": "string",
          "scope": "resource",
          "default": "",
          "description": "The api key of Groq"
        },
        "MarkdownPaste.aiModel": {
          "type": "string",
          "scope": "resource",
          "default": "llama3-8b-8192",
          "description": "The LLM model."
        },
        "MarkdownPaste.aiSysMessage": {
          "type": "string",
          "scope": "resource",
          "default": "You are responsible for converting text content into Markdown format. If the original content is HTML, ignore any color or font settings and comments, but retain tables.",
          "description": "The LLM model system message."
        },
        "MarkdownPaste.aiTemperature": {
          "type": "number",
          "scope": "resource",
          "default": 1,
          "description": "The LLM model temperature."
        },
        "MarkdownPaste.path": {
          "type": "string",
          "scope": "resource",
          "default": "${fileDirname}",
          "description": "The destination to save image file."
        },
        "MarkdownPaste.nameBase": {
          "type": "string",
          "scope": "resource",
          "default": "${datetime|yyyyMMDDHHmmss}",
          "description": "The string of default image file name."
        },
        "MarkdownPaste.namePrefix": {
          "type": "string",
          "scope": "resource",
          "default": "",
          "description": "The string prepend to the default image file name."
        },
        "MarkdownPaste.nameSuffix": {
          "type": "string",
          "scope": "resource",
          "default": "",
          "description": "The string append to the default image file name."
        },
        "MarkdownPaste.silence": {
          "type": "boolean",
          "scope": "resource",
          "default": false,
          "description": "Enable/disable showing confirm box while paste image."
        },
        "MarkdownPaste.applyAllRules": {
          "type": "boolean",
          "scope": "resource",
          "default": false,
          "description": "If true, it will apply all rules to the same text orderly, instead only the first applicable one."
        },
        "MarkdownPaste.enableHtmlConverter": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "Enable/disable converting html to markdown."
        },
        "MarkdownPaste.turndownOptions": {
          "type": "object",
          "scope": "resource",
          "default": {
            "headingStyle": "setext",
            "bulletListMarker": "-",
            "strongDelimiter": "**",
            "emDelimiter": "*",
            "preformattedCode": "true",
            "hr": "\n\n* * * *\n\n"
          },
          "description": "Use turndown options: https://github.com/mixmark-io/turndown#options"
        },
        "MarkdownPaste.enableRulesForHtml": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "Enable/disable using rules after converting html to markdown."
        },
        "MarkdownPaste.enableImgTag": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "Enable/Disable using img tag with width and height for pasting image."
        },
        "MarkdownPaste.rules": {
          "type": "array",
          "scope": "resource",
          "items": {
            "type": "object",
            "title": "Regex rule",
            "properties": {
              "regex": {
                "type": "string",
                "description": "regex string."
              },
              "options": {
                "type": "string",
                "description": "Regex options."
              },
              "replace": {
                "type": "string",
                "description": "Replace exp."
              }
            }
          },
          "default": [
            {
              "regex": "^(?:https?://)?(?:(?:(?:www\\.?)?youtube\\.com(?:/(?:(?:watch\\?.*?v=([^&\\s]+).*)|))?))",
              "options": "g",
              "replace": "[![](https://img.youtube.com/vi/$1/0.jpg)](https://www.youtube.com/watch?v=$1)"
            },
            {
              "regex": "^(https?://.*)",
              "options": "ig",
              "replace": "[]($1)"
            }
          ],
          "description": "Rules for markdown paste."
        },
        "MarkdownPaste.encodePath": {
          "type": "string",
          "scope": "resource",
          "enum": [
            "none",
            "encodeURI",
            "encodeSpaceOnly"
          ],
          "default": "encodeSpaceOnly",
          "description": "Encode path to URL-encode format. Options: none, encodeURI, encodeSpaceOnly"
        },
        "MarkdownPaste.lang_rules": {
          "type": "array",
          "scope": "resource",
          "items": {
            "type": "object",
            "title": "lang rules"
          },
          "description": "Rules for lang paste.",
          "default": [
            {
              "asciidoc": [
                {
                  "regex": "^(?:https?://)?(?:(?:(?:www\\.?)?youtube\\.com(?:/(?:(?:watch\\?.*?v=([^&\\s]+).*)|))?))",
                  "options": "g",
                  "replace": "image::https://img.youtube.com/vi/$1/0.jpg[link=\"https://www.youtube.com/watch?v=$1\"]"
                },
                {
                  "regex": "^(https?://.*)",
                  "options": "ig",
                  "replace": "image::$1[linktext,300]"
                },
                {
                  "regex": "(.*/media/)(.*)",
                  "options": "",
                  "replace": "image::$2[linktext,300]"
                }
              ]
            }
          ]
        }
      }
    },
    "commands": [
      {
        "command": "telesoho.MarkdownDownload",
        "title": "Markdown Download"
      },
      {
        "command": "telesoho.MarkdownPaste",
        "title": "Markdown Paste"
      },
      {
        "command": "telesoho.MarkdownRuby",
        "title": "Markdown Ruby"
      },
      {
        "command": "telesoho.insertMathSymbol",
        "title": "Insert latex math symbol"
      },
      {
        "command": "telesoho.MarkdownPasteCode",
        "title": "Markdown Paste Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "telesoho.MarkdownPaste",
          "when": "editorLangId == markdown",
          "group": "markdown"
        },
        {
          "command": "telesoho.MarkdownPasteCode",
          "when": "editorLangId == markdown",
          "group": "markdown"
        }
      ]
    },
    "keybindings": [
      {
        "command": "telesoho.MarkdownDownload",
        "key": "ctrl+alt+d",
        "mac": "cmd+alt+d",
        "when": "editorTextFocus"
      },
      {
        "command": "telesoho.MarkdownPaste",
        "key": "ctrl+alt+v",
        "mac": "cmd+alt+v",
        "when": "editorTextFocus && resourceLangId == markdown"
      },
      {
        "command": "telesoho.MarkdownPasteCode",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c",
        "when": "editorTextFocus && resourceLangId == markdown"
      },
      {
        "command": "telesoho.MarkdownRuby",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+alt+\\",
        "mac": "cmd+alt+\\",
        "command": "telesoho.insertMathSymbol",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/vscode-languagedetection": "^1.0.22",
    "arch": "^2.2.0",
    "groq-sdk": "^0.5.0",
    "moment": "^2.22.1",
    "shelljs": "^0.8.5",
    "turndown": "^7.1.2",
    "xclip": "file:xclip"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^2.19.0",
    "cpx": "^1.5.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.1.7",
    "mocha": "^10.2.0",
    "prettier": "2.5.1",
    "rewire": "^6.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^4.9.4"
  }
}
